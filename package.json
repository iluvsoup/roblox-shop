		"@sveltejs/adapter-vercel": "next",
		"@sveltejs/kit": "next",
		"@types/ioredis": "^4.28.10",
		"@types/jsonwebtoken": "^8.5.9",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@zerodevx/svelte-toast": "^0.8.2",
		"autoprefixer": "^10.4.13",
		"esbuild": "^0.15.16",
		"eslint": "^8.28.0",
